         <children>
            <GridPane layoutX="10.0" layoutY="15.0" prefHeight="374.0" prefWidth="378.0">
              <columnConstraints>
                <ColumnConstraints hgrow="SOMETIMES" maxWidth="152.0" minWidth="10.0" prefWidth="85.0" />
